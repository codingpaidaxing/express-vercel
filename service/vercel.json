{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": { "maxLambdaSize": "30mb" }
    },
    {
      "src": "tsconfig.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "build/app.mjs"
    }
  ],
  "env": {
    "NODE_ENV": "prod"
  }
}
