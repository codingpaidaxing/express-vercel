{
  "version": 2,
  "builds": [
    {
      "src": "tsup.config.ts",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "build/app.mjs"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "framework": {
    "name": "express",
    "version": "auto"
  }
}
